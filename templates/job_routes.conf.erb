JOB_ROUTER_ENTRIES @=jre
<% @job_routes.each do |route_name, settings| %>
[
  name = "<%=route_name -%>";
<% @job_routes_base_settings.each do |name, value| -%>
  <%=name -%> = <%=value -%>;
<% end -%>
<% settings.each do |name, value| -%>
  <%=name -%> = <%=value -%>;
<% end -%>
]<% end %>
@jre

